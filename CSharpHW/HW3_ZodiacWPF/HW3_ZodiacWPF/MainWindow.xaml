<Window x:Class="HW3_ZodiacWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HW3_ZodiacWPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid Margin="0,0,2,-21">
        <Button x:Name="Submit" Content="Check" HorizontalAlignment="Left" Margin="279,37,0,0" VerticalAlignment="Top" Width="106" Height="23" Click="Submit_Click"/>
        <TextBlock x:Name="zodiacName" HorizontalAlignment="Left" Margin="124,74,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="261"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Margin="107,37,0,0"/>
        <Image x:Name="show" HorizontalAlignment="Left" Height="192" Margin="80,106,0,0" VerticalAlignment="Top" Width="365"/>
        <Label x:Name="label" Content="Enter birth date in format DD/MM/YYYY" HorizontalAlignment="Left" Margin="34,10,0,0" VerticalAlignment="Top" Width="245" Height="27"/>
    </Grid>
</Window>
